{
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "test:workbook": "vitest run tests/unit/workbookIntegration.test.js",
    "rebuild": "npm run docs:build && npm run docs:preview",
    "update-pins": "node docs/.vitepress/utils/services/external/index.js all",
    "update-pins:spotify": "node docs/.vitepress/utils/services/external/index.js spotify",
    "update-pins:vimeo": "node docs/.vitepress/utils/services/external/index.js vimeo",
    "update-pins:youtube": "node docs/.vitepress/utils/services/external/index.js youtube",
    "update-vimeo-full": "node docs/.vitepress/utils/workflows/update-vimeo-pins.js",
    "update-vimeo-thumbnails": "node docs/.vitepress/utils/services/media/thumbnails.js --vimeo",
    "update-youtube-full": "node docs/.vitepress/utils/workflows/update-youtube-pins.js",
    "update-youtube-thumbnails": "node docs/.vitepress/utils/services/media/thumbnails.js --youtube",
    "create-session": "node docs/.vitepress/utils/maintenance/createSessionNote.js",
    "add-annotation": "node scripts/add-annotation.js",
    "auth": "node docs/.vitepress/utils/services/external/auth.js",
    "verify": "node docs/.vitepress/utils/services/external/verify.js",
    "ensure-cache": "node docs/.vitepress/utils/ensurePublicCache.js",
    "generate-favicons": "node docs/.vitepress/utils/maintenance/generateFavicons.js",
    "pins:validate-thumbnails": "node docs/.vitepress/utils/workflows/validate-pin-thumbnails.js",
    "pins:refresh-og-data": "node docs/.vitepress/utils/workflows/refresh-og-data.js",
    "pins:audit-coverage": "node -e \"import('./docs/.vitepress/utils/services/content/pins.js').then(m => m.auditVideoThumbnailCoverage())\"",
    "pins:health-check": "npm run pins:validate-thumbnails && npm run pins:audit-coverage",
    "prebuild": "npm run update-pins && npm run ensure-cache && npm run generate-favicons",
    "prebuild:minimal": "npm run ensure-cache && npm run generate-favicons",
    "verify-build": "node docs/.vitepress/utils/maintenance/verifyBuild.js",
    "upload-media": "node scripts/upload-media.js",
    "test-connectivity": "node scripts/test-connectivity.js",
    "test-unified-thumbnails": "node scripts/test-unified-thumbnails.js",
    "spotify:generate-thumbnails": "node scripts/generate-spotify-thumbnails.js",
    "spotify:audit-thumbnails": "node scripts/diagnose-pin-thumbnails.js | grep spotify",
    "spotify:update-all": "npm run spotify:fetch && npm run spotify:generate-thumbnails",
    "vimeo:import-videos": "node scripts/import-vimeo-videos-to-workbook.js",
    "vimeo:import-preview": "node scripts/import-vimeo-videos-to-workbook.js --dry-run --limit 5",
    "workbook:fix-yaml": "node scripts/fix-vimeo-yaml.js"
  },
  "dependencies": {
    "@fontsource-variable/kode-mono": "^5.2.5",
    "@fontsource/ibm-plex-mono": "^5.2.5",
    "@fontsource/ibm-plex-sans": "^5.2.6",
    "@fontsource/ibm-plex-serif": "^5.2.6",
    "@fontsource/monaspace-argon": "^5.2.5",
    "lucide-vue-next": "^0.513.0",
    "vitepress": "^1.6.3"
  },
  "devDependencies": {
    "@fontsource-variable/source-sans-3": "^5.1.1",
    "@playwright/test": "^1.42.0",
    "@testing-library/vue": "^8.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.4.0",
    "cheerio": "^1.0.0",
    "chokidar": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.0",
    "googleapis": "^148.0.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^24.1.3",
    "node-fetch": "^2.7.0",
    "open": "^10.1.2",
    "sharp": "^0.33.2",
    "spotify-web-api-node": "^5.0.2",
    "uuid": "^9.0.1",
    "vimeo": "^2.3.1",
    "vitest": "^1.4.0"
  },
  "type": "module"
}
