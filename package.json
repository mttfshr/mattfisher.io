    "auth": "node docs/.vitepress/utils/services/external/auth.js",
    "verify": "node docs/.vitepress/utils/services/external/verify.js",
    "ensure-cache": "node docs/.vitepress/utils/ensurePublicCache.js",
    "generate-favicons": "node docs/.vitepress/utils/maintenance/generateFavicons.js",
    "prebuild": "npm run update-pins && npm run generate-og-cache && npm run ensure-cache && npm run generate-favicons",
    "prebuild:minimal": "npm run ensure-cache && npm run generate-favicons",
    "verify-build": "node docs/.vitepress/utils/maintenance/verifyBuild.js",
    "upload-media": "node scripts/upload-media.js",
