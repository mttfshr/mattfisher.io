# Spotify API credentials
SPOTIFY_CLIENT_ID=your_client_id_from_dashboard
SPOTIFY_CLIENT_SECRET=your_client_secret_from_dashboard
SPOTIFY_REFRESH_TOKEN=your_refresh_token_from_auth_process

# Vimeo API credentials
# Get your access token from https://developer.vimeo.com/apps
# See docs/.vitepress/utils/connectors/VIMEO_GUIDE.md for setup instructions
VIMEO_ACCESS_TOKEN=your_vimeo_access_token

# YouTube API credentials
# Set up OAuth 2.0 credentials in Google Cloud Console
# See docs/.vitepress/utils/connectors/YOUTUBE_GUIDE.md for setup instructions
YOUTUBE_CLIENT_ID=your_client_id_from_google_cloud_console
YOUTUBE_CLIENT_SECRET=your_client_secret_from_google_cloud_console
YOUTUBE_REFRESH_TOKEN=your_refresh_token_from_auth_process
